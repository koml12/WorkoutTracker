<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.mohit31.workouttracker">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Activities.TimerActivity">
        </activity>
        <activity
                android:name=".Activities.WorkoutViewActivity"
                android:label="@string/title_activity_workout_view"
                android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".Activities.AddExerciseActivity">
        </activity>
        <activity android:name=".Activities.StartWorkoutActivity">
        </activity>
        <activity android:name=".Activities.EditExerciseActivity">
        </activity>
        <activity
                android:name=".Activities.WorkoutListActivity"
                android:label="@string/title_activity_workout_list"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activities.AddWorkoutActivity">
        </activity>
    </application>

</manifest>